{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "hebi",
      "camelcase": "Hebi",
      "title": "Hebi",
      "scope": "source.hebi",
      "file-types": [
        "hebi"
      ],
      "injection-regex": "^hebi$",
      "class-name": "TreeSitterHebi"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Hebi grammar for tree-sitter",
    "authors": [
      {
        "name": "jprochazk"
      }
    ],
    "links": {
      "repository": "https://github.com/jprochazk/tree-sitter-hebi"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}